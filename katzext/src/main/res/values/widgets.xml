<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="compoundDrawableWidth" format="dimension|reference"/>
    <attr name="compoundDrawableHeight" format="dimension|reference"/>
    <attr name="extendBackground1" format="reference|color"/>
    <attr name="extendBackground2" format="reference|color"/>
    <attr name="extendEnabled" format="boolean"/>
    <attr name="extendActivated" format="boolean"/>

    <declare-styleable name="ExtendCheckBox">
        <attr name="compoundDrawableWidth"/>
        <attr name="compoundDrawableHeight"/>
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
    </declare-styleable>
    <declare-styleable name="ExtendEditText">
        <attr name="compoundDrawableWidth"/>
        <attr name="compoundDrawableHeight"/>
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
        <attr name="extendInputMask" format="string|reference"/>
        <attr name="extendMaskChar" format="string|reference"/>
    </declare-styleable>
    <declare-styleable name="ExtendRadioButton">
        <attr name="compoundDrawableWidth"/>
        <attr name="compoundDrawableHeight"/>
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
    </declare-styleable>
    <declare-styleable name="ExtendTextView">
        <attr name="compoundDrawableWidth"/>
        <attr name="compoundDrawableHeight"/>
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
    </declare-styleable>

    <declare-styleable name="ExtendFrameLayout">
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
    </declare-styleable>

    <declare-styleable name="ExtendLinearLayout">
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
    </declare-styleable>

    <declare-styleable name="ExtendImageView">
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
        <attr name="touchZoom" format="boolean"/>
        <attr name="maxZoom" format="float"/>
        <attr name="minZoom" format="float"/>
    </declare-styleable>

    <declare-styleable name="ExtendProgressBar">
        <attr name="extendTint" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="extendForceCenterMode" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="ExtendSpinner">
        <attr name="extendPopupBackground" format="reference|color"/>
        <attr name="extendPopupElevation" format="reference|dimension"/>
        <attr name="extendPopupMinHeight" format="reference|dimension"/>
        <attr name="extendPopupAdditionWidth" format="reference|dimension"/>
        <attr name="extendPopupVerticalOffset" format="reference|dimension"/>
        <attr name="extendPopupHorizontalOffset" format="reference|dimension"/>
        <attr name="toolsItem" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ExtendConstraintLayout">
        <attr name="extendIgnoreVerticalBounds" format="reference|boolean"/>
        <attr name="extendIgnoreHorizontalBounds" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="offsetHorizontal" format="dimension|reference"/>
        <attr name="offsetVertical" format="dimension|reference"/>
        <attr name="lineHeight" format="dimension|reference"/>
        <attr name="lineGravity" format="enum">
            <enum name="center" value="0"/>
            <enum name="top" value="1"/>
            <enum name="bot" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ExtendRelativeLayout">
        <attr name="extendBackground1"/>
        <attr name="extendBackground2"/>
        <attr name="extendEnabled"/>
        <attr name="extendActivated"/>
    </declare-styleable>

    <declare-styleable name="SlideMenuLayout_Layout">
        <attr name="slided" format="boolean|reference"/>
        <attr name="slideEnabled" format="boolean|reference"/>
        <attr name="slideSelfEnabled" format="boolean|reference"/>
        <attr name="slideSizeAmount" format="dimension|reference"/>
        <attr name="slideMinDistance" format="dimension|reference"/>
        <attr name="slideAutoFinishDuration" format="integer|reference"/>
        <attr name="slideDirection" format="enum">
            <enum name="fromLeft" value="1"/>
            <enum name="fromRight" value="2"/>
            <enum name="fromTop" value="3"/>
            <enum name="fromBottom" value="4"/>
        </attr>
        <attr name="scrollBehavior">
            <flag name="none" value="0"/>
            <flag name="auto" value="1"/>
        </attr>
        <attr name="nestedScrollBehavior">
            <flag name="openBeforeNestedScroll" value="1"/>
            <flag name="openAfterNestedScroll" value="2"/>
            <flag name="closeBeforeNestedScroll" value="4"/>
            <flag name="closeAfterNestedScroll" value="8"/>
        </attr>
        <attr name="slideZoneForId" format="reference"/>
        <attr name="slideAnchorForId" format="reference"/>
    </declare-styleable>

</resources>